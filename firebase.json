{
  "hosting": {
    "public": "out",
    "site": "souravkumarpandit",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "destination": "/admin/[**]/index.html",
        "regex": "^/job/([^/]+?)(?:/)?$"
      }
    ]
  }
}